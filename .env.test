# .env.test
# --- Секция 1: Идентификация и Связь с Core Logic ---
CORE_LOGIC_URL="http://mock-core-logic:8000"
AGENT_SECRET_TOKEN="test-token-12345"
AGENT_PUBLIC_URL="http://agent:8080"

# --- Секция 2: Подключение к Базе Данных ---
# Эти данные будут переопределены в docker-compose.test.yml,
# но полезно иметь их здесь для ясности.
DB_DIALECT="postgresql"
DB_HOST="test-db" # Важно: имя сервиса из docker-compose
DB_PORT=5432      # Важно: внутренний порт Docker
DB_USER="testuser"
DB_PASSWORD="testpassword"
DB_NAME="testdb"
DB_SSL_MODE="prefer"

# --- Секция 3: Настройки Веб-сервера Агента ---
AGENT_HOST="0.0.0.0"
AGENT_PORT=8080
